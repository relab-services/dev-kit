    this.data = e, this.name = n, Me.prototype.hasOwnProperty("tree") || Object.defineProperty(Me.prototype, "tree", { get() {
      return kn(this);
    } }), this.parser = t, this.extension = [
      wo.of(this),
      Me.languageData.of((o, i, a) => {
        let l = cg(o, i, a), c = l.type.prop(Xn);
        if (!c)
          return [];
