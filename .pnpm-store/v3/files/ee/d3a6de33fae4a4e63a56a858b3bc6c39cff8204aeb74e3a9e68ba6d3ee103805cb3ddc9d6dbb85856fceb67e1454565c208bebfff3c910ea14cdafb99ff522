const variables = () => ViewPlugin.fromClass(
  class {
    constructor(view) {
      __publicField(this, "variables");
      this.variables = variableHighlighterDecoration.createDeco(view);
    }
    update(update) {
      this.variables = variableHighlighterDecoration.updateDeco(
        update,
        this.variables
      );
    }
  },
  {
    decorations: (instance) => instance.variables,
    provide: (plugin) => EditorView.atomicRanges.of(
      (view) => {
        var _a2;
        return ((_a2 = view.plugin(plugin)) == null ? void 0 : _a2.variables) || Decoration.none;
      }
    )
  }
);
const hasProvider = (params) => "provider" in params && !!toValue$1(params.provider);
const selectAllKeyBinding = {
  key: "Mod-a",
  run: (view) => {
    view.dispatch({
      selection: { anchor: 0, head: view.state.doc.length },
      scrollIntoView: false
    });
    return true;
  }
};
const useCodeMirror = (params) => {
  const codeMirror = ref(null);
  watch(
    params.codeMirrorRef,
    () => {
      var _a2;
      (_a2 = codeMirror.value) == null ? void 0 : _a2.destroy();
      mountCodeMirror();
    },
    { immediate: true }
  );
  onBeforeUnmount(() => {
    var _a2;
    return (_a2 = codeMirror.value) == null ? void 0 : _a2.destroy();
  });
  function mountCodeMirror() {
    if (params.codeMirrorRef.value) {
      const provider = hasProvider(params) ? toValue$1(params.provider) : null;
      const extensions = getCodeMirrorExtensions({
        ...extensionConfig.value,
        provider
      });
      codeMirror.value = new EditorView({
        parent: params.codeMirrorRef.value,
        extensions
      });
      if (!hasProvider(params))
        setCodeMirrorContent(toValue$1(params.content));
    }
  }
  const extensionConfig = computed(() => ({
    onChange: params.onChange,
    onBlur: params.onBlur,
    onFocus: params.onFocus,
    language: toValue$1(params.language),
    classes: toValue$1(params.classes),
    readOnly: toValue$1(params.readOnly),
