function Ga({ total: r, ranges: e }, t) {
  if (t <= 0)
    return e[0].from;
  if (t >= 1)
    return e[e.length - 1].to;
  let s = Math.floor(r * t);
  for (let n = 0; ; n++) {
    let { from: o, to: i } = e[n], a = i - o;
    if (s <= a)
      return o + s;
    s -= a;
  }
}
function Ja(r, e) {
  let t = 0;
  for (let { from: s, to: n } of r.ranges) {
    if (e <= n) {
      t += e - s;
      break;
    }
    t += n - s;
  }
  return t / r.total;
}
