        return;
      else {
        const provider = hasProvider(params) ? toValue$1(params.provider) : null;
        const extensions = getCodeMirrorExtensions({
          ...extensionConfig.value,
          provider
        });
        codeMirror.value.dispatch({
          effects: StateEffect.reconfigure.of(extensions)
        });
      }
    },
    { immediate: true }
  );
  const setCodeMirrorContent = (newValue = "") => {
    if (!codeMirror.value)
      return;
    if (codeMirror.value.state.doc.toString() === newValue)
      return;
    codeMirror.value.dispatch({
      changes: {
        from: 0,
        to: codeMirror.value.state.doc.length,
        insert: newValue
      },
      selection: {
        anchor: Math.min(
          codeMirror.value.state.selection.main.anchor,
          newValue.length
        )
      }
    });
  };
  watch(
    () => toValue$1(params.content),
    () => {
      if (hasProvider(params))
        return;
      setCodeMirrorContent(toValue$1(params.content));
    },
    { immediate: true }
  );
  return {
    /** Replaces the current content with the given value. */
    setCodeMirrorContent,
    /** Codemirror instance */
    codeMirror
  };
};
const languageExtensions = {
  html,
  json,
  yaml,
  css
};
function getCodeMirrorExtensions({
  onChange,
  onBlur,
  onFocus,
  provider,
  language: language2,
  classes = [],
  readOnly: readOnly2 = false,
  lineNumbers: lineNumbers$1 = false,
  withVariables = false,
  disableEnter = false,
  withoutTheme = false,
  additionalExtensions = []
}) {
  const extensions = [
    highlightSpecialChars(),
    syntaxHighlighting(defaultHighlightStyle, { fallback: true }),
    EditorView.theme({
      ".cm-line": {
        lineHeight: "20px"
      },
      ".cm-gutterElement": {
        lineHeight: "20px"
      }
    }),
    // Listen to updates
    EditorView.updateListener.of((v) => {
      if (!v.docChanged)
        return;
      onChange == null ? void 0 : onChange(v.state.doc.toString());
    }),
    EditorView.domEventHandlers({
      blur: (event, view) => {
        onBlur == null ? void 0 : onBlur(view.state.doc.toString());
      },
      focus: (event, view) => {
        onFocus == null ? void 0 : onFocus(view.state.doc.toString());
      }
    }),
    // Add Classes
    EditorView.editorAttributes.of({ class: classes.join(" ") }),
    ...additionalExtensions
  ];
  if (provider)
    extensions.push(provider);
  if (!withoutTheme)
    extensions.push(customTheme);
  if (readOnly2) {
    extensions.push(EditorView.editable.of(false));
  } else {
    extensions.push(
      indentOnInput(),
      bracketMatching(),
      autocompletion(),
      closeBrackets(),
      keymap.of([
        ...completionKeymap,
        ...closeBracketsKeymap,
        indentWithTab,
        selectAllKeyBinding
      ])
    );
  }
  if (language2 && languageExtensions[language2]) {
    extensions.push(languageExtensions[language2]());
  }
  if (lineNumbers$1)
    extensions.push(lineNumbers());
  if (withVariables)
    extensions.push(variables());
  if (disableEnter) {
    extensions.push(
      keymap.of([
        {
          key: "Enter",
          run: () => {
            return true;
          }
        },
        {
          key: "Ctrl-Enter",
          mac: "Cmd-Enter",
          run: () => {
            return true;
          }
        },
        {
          key: "Shift-Enter",
          run: () => {
            return true;
          }
        }
      ])
    );
  }
  return extensions;
}
