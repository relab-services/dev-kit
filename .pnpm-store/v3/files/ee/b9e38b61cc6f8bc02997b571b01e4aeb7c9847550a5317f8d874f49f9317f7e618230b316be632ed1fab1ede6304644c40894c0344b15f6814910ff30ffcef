  }
  nextInner(e, t) {
    for (this.done = this.lineBreak = !1; ; ) {
      let s = this.nodes.length - 1, n = this.nodes[s], o = this.offsets[s], i = o >> 1, a = n instanceof rt ? n.text.length : n.children.length;
