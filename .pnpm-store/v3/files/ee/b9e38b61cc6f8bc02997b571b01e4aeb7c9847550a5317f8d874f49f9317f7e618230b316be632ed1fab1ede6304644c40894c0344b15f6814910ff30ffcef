      return r;
    throw new Error("Invalid JSON pointer.");
  } else if (Array.isArray(r)) {
    for (const e of r)
      if (typeof e != "string" && typeof e != "number")
        throw new Error("Invalid JSON pointer. Must be of type string or number.");
    return r;
  }
  throw new Error("Invalid JSON pointer.");
}
