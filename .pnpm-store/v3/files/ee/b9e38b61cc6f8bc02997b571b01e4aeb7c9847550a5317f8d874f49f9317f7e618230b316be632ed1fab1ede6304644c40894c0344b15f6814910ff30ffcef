function Zm(r, e, t, s) {
  let n = r.cursor(), o = [];
  if (!n.firstChild())
    return o;
  if (t != null) {
    for (let i = !1; !i; )
      if (i = n.type.is(t), !n.nextSibling())
        return o;
  }
  for (; ; ) {
    if (s != null && n.type.is(s))
      return o;
    if (n.type.is(e) && o.push(n.node), !n.nextSibling())
      return s == null ? o : [];
  }
}
function Ud(r, e, t = e.length - 1) {
  for (let s = r.parent; t >= 0; s = s.parent) {
    if (!s)
      return !1;
    if (!s.type.isAnonymous) {
      if (e[t] && e[t] != s.name)
        return !1;
      t--;
    }
  }
  return !0;
}
