      s.add(n.from, n.to, n.value);
    return s.finish();
  }
  /**
  Join an array of range sets into a single set.
  */
  static join(e) {
    if (!e.length)
      return Re.empty;
    let t = e[e.length - 1];
    for (let s = e.length - 2; s >= 0; s--)
      for (let n = e[s]; n != Re.empty; n = n.nextLayer)
        t = new Re(n.chunkPos, n.chunk, t, Math.max(n.maxPoint, t.maxPoint));
    return t;
  }
