  );
  const url = typeof (server2 == null ? void 0 : server2.url) === "string" ? replaceVariables(server2 == null ? void 0 : server2.url, variables2) : server2 == null ? void 0 : server2.url;
  return url;
}
function humanDiff(unixTimestamp) {
  const seconds = Math.floor(((/* @__PURE__ */ new Date()).getTime() - unixTimestamp) / 1e3);
  if (seconds < 45) {
    return "just now";
  }
  if (seconds < 60) {
    return `${seconds} seconds ago`;
  }
  const minutes = Math.floor(seconds / 60);
  if (minutes < 60) {
    return `${minutes} minute${minutes === 1 ? "" : "s"} ago`;
  }
  const hours = Math.floor(minutes / 60);
  if (hours < 24) {
    return `${hours} hour${hours === 1 ? "" : "s"} ago`;
  }
  const days = Math.floor(hours / 24);
  if (days < 30) {
    return `${days} day${days === 1 ? "" : "s"} ago`;
  }
  const months = Math.floor(days / 30);
  if (months < 12) {
    return `${months} month${months === 1 ? "" : "s"} ago`;
  }
  return "more than a year ago";
}
function mapFromArray(arr, key, valueKey) {
  const obj = {};
  arr.forEach((entry) => {
    obj[entry[key]] = entry[valueKey];
  });
  return obj;
}
const normalizeHeaders = (headers) => Object.fromEntries(
  Object.entries(headers ?? {}).map(([key, value]) => [key.toLowerCase(), value])
);
const normalizePath = (path) => {
  if (typeof path !== "string") {
    return "";
  }
  let normalizedPath = path.trim();
  if (normalizedPath.length > 1 && normalizedPath.startsWith("/")) {
    normalizedPath = normalizedPath.slice(1);
  }
  return normalizedPath;
};
const defaultRequestMethod = "GET";
const normalizeRequestMethod = (method) => {
  if (typeof method !== "string") {
    console.warn(
      `[sendRequest] Request method is not a string. Using ${defaultRequestMethod} as the default.`
    );
    return defaultRequestMethod;
  }
  const normalizedMethod = method.trim().toUpperCase();
  if (!isRequestMethod(normalizedMethod)) {
    console.warn(
      `[sendRequest] ${method} is not a valid request method. Using ${defaultRequestMethod} as the default.`
    );
    return defaultRequestMethod;
  }
  return normalizedMethod;
};
const normalizeUrl = (url) => {
  if (typeof url !== "string") {
    console.warn(
      `[sendRequest] URL is not a string. Using an empty string as the default.`
    );
    return "";
  }
  let sanitizedUrl = url;
  if (!sanitizedUrl.startsWith("http")) {
    console.warn(
      `[sendRequest] URL does not start with http. Adding http:// as the default prefix.`
    );
    sanitizedUrl = `http://${sanitizedUrl}`;
  }
  return sanitizedUrl.trim();
};
function enable(items) {
  return (items ?? []).map((item) => ({ ...item, enabled: true }));
}
const prepareClientRequestConfig = (configuration) => {
  var _a2, _b;
  const { request } = configuration;
  const { authentication: authentication2 } = useAuthenticationStore();
  const {
    openApi: { operation, globalSecurity }
  } = useOpenApiStore();
