function wb(r, e, t, s) {
  switch (r) {
    case -2:
      return t < e;
    case -1:
      return s >= e && t < e;
    case 0:
      return t < e && s > e;
    case 1:
      return t <= e && s > e;
    case 2:
      return s > e;
    case 4:
      return !0;
  }
}
function Zi(r, e, t, s) {
  for (var n; r.from == r.to || (t < 1 ? r.from >= e : r.from > e) || (t > -1 ? r.to <= e : r.to < e); ) {
