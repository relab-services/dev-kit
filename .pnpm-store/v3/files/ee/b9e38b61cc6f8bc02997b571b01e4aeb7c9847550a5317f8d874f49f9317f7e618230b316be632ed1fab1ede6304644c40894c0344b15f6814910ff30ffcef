      else
        throw new Error(`Expected YAML collection at ${s}. Remaining path: ${n}`);
    }
  }
  /**
   * Removes a value from the collection.
   * @returns `true` if the item was found and removed.
   */
  deleteIn(e) {
    const [t, ...s] = e;
    if (s.length === 0)
      return this.delete(t);
    const n = this.get(t, !0);
    if (Ut(n))
      return n.deleteIn(s);
