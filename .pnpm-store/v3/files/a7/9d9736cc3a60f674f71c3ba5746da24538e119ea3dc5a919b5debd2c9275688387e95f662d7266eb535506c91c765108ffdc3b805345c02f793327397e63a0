      color: var(--scalar-color-3);
      font-family: var(--scalar-font);
}
input::-webkit-input-placeholder {
      color: var(--scalar-color-3);
      font-family: var(--scalar-font);
}
    /** Remove yellow/blue autofill indicator */
input:-webkit-autofill {
      background-clip: text !important;
}
}
