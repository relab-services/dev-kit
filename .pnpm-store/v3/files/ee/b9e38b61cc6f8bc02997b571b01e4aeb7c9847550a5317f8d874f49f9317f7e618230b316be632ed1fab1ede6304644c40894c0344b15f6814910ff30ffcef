      if (!e.some(({ from: o, to: i }) => n >= o && n <= i)) {
        let { from: o, to: i } = this.lineBlockAt(n);
        e.push(new Wa(o, i));
      }
    }
    this.viewports = e.sort((s, n) => s.from - n.from), this.scaler = this.heightMap.height <= 7e6 ? Lm : new D$(this.heightOracle, this.heightMap, this.viewports);
  }
