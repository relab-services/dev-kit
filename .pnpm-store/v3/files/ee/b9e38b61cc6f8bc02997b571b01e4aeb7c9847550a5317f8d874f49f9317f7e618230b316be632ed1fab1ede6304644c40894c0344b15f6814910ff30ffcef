  o.source === "" && n(e, "BAD_ALIAS", "Alias cannot be an empty string"), o.source.endsWith(":") && n(e + t.length - 1, "BAD_ALIAS", "Alias ending in : is ambiguous", !0);
  const i = e + t.length, a = Ea(s, i, r.strict, n);
