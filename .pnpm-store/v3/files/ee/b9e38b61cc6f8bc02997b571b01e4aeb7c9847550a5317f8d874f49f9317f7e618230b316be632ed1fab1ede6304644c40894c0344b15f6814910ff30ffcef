      for (let h = 0; h < a; h++)
        o[h] = o[h].map(u);
      let d = n.mapDesc(c, !0);
      o.push(l.range.map(d)), n = n.compose(u), i = pt.mapEffects(i, u).concat(pt.mapEffects(no(l.effects), d));
