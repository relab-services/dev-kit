function redirectToProxy(proxy, url) {
  const newUrl = new URL(url);
  newUrl.href = proxy;
  newUrl.searchParams.append("scalar_url", url);
  return newUrl.toString();
}
function isRequestToLocalhost(url) {
  const { hostname } = new URL(url);
  const listOfLocalUrls = ["localhost", "127.0.0.1", "[::1]"];
  return listOfLocalUrls.includes(hostname);
}
async function sendRequest(request, proxyUrl) {
  var _a2;
  const method = normalizeRequestMethod(request.type);
  const headers = mapFromArray(
    (request.headers ?? []).filter((header) => header.enabled),
    "name",
    "value"
  );
  const url = normalizeUrl(request.url);
  const path = normalizePath(request.path);
  const [urlWithPath, ...urlQueryString] = concatenateUrlAndPath(
    url,
    path
  ).split("?");
  const renderedUrl = replaceVariables(
    urlWithPath,
    mapFromArray(
      (request.variables ?? []).filter((variable2) => variable2.enabled),
      "name",
      "value"
    )
  );
  const queryStringsFromUrl = [];
  urlQueryString.forEach((queryString2) => {
    new URLSearchParams(queryString2 ?? "").forEach((value, name2) => {
      queryStringsFromUrl.push({
        name: name2,
        value,
        enabled: true
      });
    });
  });
  const queryString = new URLSearchParams(
    // TODO: No type-casting
    mapFromArray(
      [
        ...(request.query ?? []).filter((query) => query.enabled),
        ...queryStringsFromUrl
      ],
      "name",
      "value"
    )
  ).toString();
  const completeUrl = `${renderedUrl}${queryString ? "?" + queryString : ""}`;
  const auth = {
    type: "none"
  };
  const startTime = Date.now();
  if (request.cookies && ((_a2 = request.cookies) == null ? void 0 : _a2.length) > 0) {
    const cookies = mapFromArray(
      (request.cookies ?? []).filter((cookie) => cookie.enabled),
      "name",
      "value"
    );
    headers.Cookie = Object.keys(cookies).map((key) => `${key}=${cookies[key]}`).join("; ");
  }
  const requestConfig = {
    method,
    url: completeUrl,
    auth,
    headers,
    data: request.body
  };
  const shouldUseProxy = proxyUrl && !isRequestToLocalhost(requestConfig.url);
  const axiosRequestConfig = {
    method: requestConfig.method,
    url: shouldUseProxy ? redirectToProxy(proxyUrl, requestConfig.url) : requestConfig.url,
    headers: requestConfig.headers,
    data: requestConfig.data
  };
  if (headers.cookies) {
    axiosRequestConfig.withCredentials = true;
  }
  if (shouldUseProxy) {
    console.info(
      `${requestConfig.method} ${requestConfig.url} (proxy: ${proxyUrl})`
    );
  } else {
    console.info(`${requestConfig.method} ${requestConfig.url}`);
  }
  const response = await axios(axiosRequestConfig).then((result) => ({
    ...result,
    statusCode: result.status,
    data: result.data,
    error: false
  })).catch((error) => {
    var _a3, _b;
    const { response: errorResponse } = error;
    console.error("ERROR", error);
    return {
      data: error.code ?? error.message,
      ...errorResponse,
      statusCode: (errorResponse == null ? void 0 : errorResponse.status) ?? 0,
      error: {
        message: ((_a3 = errorResponse == null ? void 0 : errorResponse.data) == null ? void 0 : _a3.message) ?? error.message,
        stack: ((_b = errorResponse == null ? void 0 : errorResponse.data) == null ? void 0 : _b.stack) ?? error.stack
      }
    };
  });
  return {
    sentTime: Date.now(),
    request: {
      ...request,
      type: method,
      url,
      path
    },
    response: {
      ...response,
      duration: Date.now() - startTime
    },
    responseId: nanoid()
  };
}
